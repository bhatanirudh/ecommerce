                            <i><img src="images/star.png"/></i>

                            <form action="/addToCart" method="post">
                                <input type="hidden" name="id" value="<%= item._id %>" >
                                <input type="hidden" name="quantity" value="1" >
                                <input type="submit" value="+ Add To Cart">
                            </form>
                        </div>
                    </div>

